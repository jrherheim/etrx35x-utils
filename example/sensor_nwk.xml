<rfconfig>
  <node type="COO">
    <!-- Link Key -->
    <reg name="S09" password="password" overwrite="y" type="string">
      12341234123412341234123412341234
    </reg>
    <!-- Настройки безопасности -->
    <reg name="S0A" password="password" overwrite="n" type="hex16">
      0114
    </reg>
    <!-- Выходная мощность передатчика -->
    <reg name="S01" overwrite="y" type="int">
      3
    </reg>
  </node>
  <node type="SED">
    <!-- Link Key -->
    <reg name="S09" password="password" overwrite="y" type="string">
      12341234123412341234123412341234
    </reg>
    <!-- Настройки безопасности -->
    <reg name="S0A" password="password" overwrite="n" type="hex16">
      0114
    </reg>
    <!-- Выходная мощность передатчика -->
    <reg name="S01" overwrite="y" type="int">
      3
    </reg>
    <!-- Включение альтернативной функциональности PB0 -->
    <reg name="S15" overwrite="n" type="hex32">
      2000
    </reg>
    <!-- Конфигурирование портов ввода/вывода -->
    <reg name="S13" overwrite="y" type="hex32">
      00FF7EFB
    </reg>
    <reg name="S16" overwrite="y" type="hex32">
      00008204
    </reg>
    <reg name="S18" overwrite="y" type="hex32">
      00FD58E7
    </reg>
  </node>
</rfconfig>